.metadata__column--manage
  a
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    text-transform uppercase
    font-weight normal
    font-size 11px
    &:before
      content attr(data-static)
  svg
    vertical-align middle
    height 18px
    width 18px
    margin-bottom 2px
    margin-left 5px
    fill lightgray
  &:hover a:before
    content attr(data-hover)
  &:hover svg
    color hovergray
    fill hovergray

