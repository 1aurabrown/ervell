.OnboardingDemo
  display flex
  justify-content center
  align-items center
  flex 1
  min-width 0
  width 100%
  height 100%
  margin 1em auto
  overflow hidden

  > iframe
    max-width 100%
    max-height 100%

  &__aspect
    width 100%
    // Applies ratio-box & width

    > iframe
      position absolute
      top 0
      left 0
      width 100%
      height 100%
